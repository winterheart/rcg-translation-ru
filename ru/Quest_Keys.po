msgid "Quest Complete:"
msgstr "Задание выполнено:"

msgctxt "QUEST_COMPLETE_MISSION"
msgid "Mission Complete:"
msgstr "Миссия пройдена:"

msgctxt "QUEST_FAILED_QUEST"
msgid "Quest Failed:"
msgstr "Задание провалено:"

msgctxt "QUEST_FAILED_MISSION"
msgid "Mission Failed:"
msgstr "Миссия не пройдена:"

msgctxt "QUEST_NAME_LETS_BEAT_IT"
msgid "Let's Beat It!"
msgstr "Забьём на это!"

msgctxt "QUEST_DESC_LETS_BEAT_IT"
msgid "Find a way out of school."
msgstr "Найди выход из школы."
